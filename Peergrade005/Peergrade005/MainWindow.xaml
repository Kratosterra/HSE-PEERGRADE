        <TextBlock x:Name="DistanceText" Grid.Column="6" Grid.Row="8" Grid.ColumnSpan="2" FontWeight="Normal" FontStyle="Italic" Text="Расстояние: 1" TextAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden" />
        <Image x:Name="DistanceImage" Grid.Column="5" Grid.Row="8" Source="/distance.png" Stretch="Uniform" StretchDirection="Both" Margin="0,0,0,5" HorizontalAlignment="Right" VerticalAlignment="Top" ScrollViewer.VerticalScrollBarVisibility="Disabled" UseLayoutRounding="True" Visibility="Hidden"/>
        <Slider x:Name="DistanceSlider" Style="{DynamicResource Horizontal_Slider}" SmallChange="1" Grid.Column="8" Grid.Row="8" Grid.ColumnSpan="2" Minimum="1" Maximum="80" Visibility="Hidden" ValueChanged="DistanceSlider_ValueChanged" Margin="0,0,6,0" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>

        <GroupBox x:Name="GradientSettingsBox" Grid.Column="5" Header="Настройкa цветов градиента" Grid.Row="10" Grid.ColumnSpan ="5" Grid.RowSpan ="3" BorderBrush="Black" FontWeight="Bold" Margin="3,3,3,1" Padding="3,3,3,3"/>
        <TextBlock x:Name="StartColourText" Grid.Column="6" Grid.Row="11" Grid.ColumnSpan="1" FontWeight="Normal" FontStyle="Italic" Text="(0,0,0)" TextAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" />
        <Slider x:Name="StartColourRSlider" Style="{DynamicResource Horizontal_Slider}" Grid.Column="7" Grid.Row="11" Grid.ColumnSpan="1" Minimum="0" Maximum="255" SmallChange="1" ValueChanged="StartColourRSlider_ValueChanged" Margin="0,0,10,0" HorizontalAlignment="Stretch" VerticalAlignment="Center" MinWidth="0" MinHeight="0"/>
        <Slider x:Name="StartColourGSlider" Style="{DynamicResource Horizontal_Slider}" Grid.Column="8" Grid.Row="11" Grid.ColumnSpan="1" Minimum="0" Maximum="255" SmallChange="1" ValueChanged="StartColourGSlider_ValueChanged" Margin="0,0,10,0" VerticalAlignment="Center" MinWidth="0" MinHeight="0"/>
        <Slider x:Name="StartColourBSlider" Style="{DynamicResource Horizontal_Slider}" Grid.Column="9" Grid.Row="11" Grid.ColumnSpan="1" Minimum="0" Maximum="255" Margin="0,0,10,0" SmallChange="1" ValueChanged="StartColourBSlider_ValueChanged" HorizontalAlignment="Stretch" VerticalAlignment="Center" MinWidth="0" MinHeight="0"/>
        <TextBlock x:Name="EndColourText" Grid.Column="6" Grid.Row="12" Grid.ColumnSpan="1" FontWeight="Normal" FontStyle="Italic" Text="(0,0,0)" TextAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" />
        <Ellipse x:Name="ShowStartColor" Grid.Column="5" Grid.Row="11" Fill="Black" Margin="68,5,4,5" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        <Ellipse x:Name="ShowEndColor" Grid.Column="5" Grid.Row="12" Fill="Black" Margin="68,3,4,7"/>
        <Slider x:Name="EndColourRSlider" Style="{DynamicResource Horizontal_Slider}" Grid.Column="7" Grid.Row="12" Grid.ColumnSpan="1" Minimum="0" Maximum="255" SmallChange="1" ValueChanged="EndColourRSlider_ValueChanged" Margin="0,0,10,0" VerticalAlignment="Center" MinWidth="0" MinHeight="0"/>
        <Slider  x:Name="EndColourGSlider" Style="{DynamicResource Horizontal_Slider}" Grid.Column="8" Grid.Row="12" Grid.ColumnSpan="1" Minimum="0" Maximum="255" SmallChange="1" ValueChanged="EndColourGSlider_ValueChanged" Margin="0,0,10,0" VerticalAlignment="Center" MinWidth="0" MinHeight="0"/>
        <Slider x:Name="EndColourBSlider" Style="{DynamicResource Horizontal_Slider}" Grid.Column="9" Grid.Row="12" Grid.ColumnSpan="1" Minimum="0" Maximum="255" Margin="0,0,10,0" SmallChange="1" ValueChanged="EndColourBSlider_ValueChanged" VerticalAlignment="Center" MinWidth="0" MinHeight="0"/>

        <Button x:Name="CreateFractalButton" Style="{DynamicResource ButtonStyleMain}" Content="Построить фрактал" Grid.Row="14" Grid.Column="5" Grid.ColumnSpan="5" BorderThickness="1,1,1,1" BorderBrush="Black" Foreground="White" FontWeight="Bold" FontStyle="Normal" Background="#FF621778" Padding="3,3,3,3" Margin="3,3,3,3" Grid.RowSpan="2" Click="CreateFractalButton_Click" FontSize="20" />
    </Grid>
</Window>
